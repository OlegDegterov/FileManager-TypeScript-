{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2019"
    ],
    "jsx": "react",
    "module": "esnext",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "sourceMap": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true
  },

}
